{
  "title": "Tweet Emotions",
  "description": "The data is a collection of tweets annotated with the emotions behind them.",
  "modelCategory": "text_classification_base",
  "schemaVersion": 1.0,
  "inputDataFormat": "CSV",
  "encoding": "utf-8",
  "id": {
    "name": "tweet_id",
    "description": "Unique identifier for each sample in the dataset."
  },
  "target": {
    "name": "sentiment",
    "description": "..",
    "classes": [
      "anger",
      "boredom",
      "empty",
      "enthusiasm",
      "fun",
      "happiness",
      "hate",
      "love",
      "neutral",
      "relief",
      "sadness",
      "surprise",
      "worry"
    ]
  },
  "features": [
    {
      "name": "content",
      "description": "The text to be classified",
      "dataType": "TEXT",
      "example": "Good Morning",
      "nullable": false
    }
  ]
}